{
  "name": "user-service",
  "version": "1.0.0",
  "description": "User management microservice for Spatial Tag platform",
  "main": "dist/src/app.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "commonjs",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "node dist/src/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "test": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":90,\"functions\":90,\"lines\":90,\"statements\":90}}'",
    "lint": "eslint . --ext .ts --config .eslintrc.json",
    "proto:gen": "protoc --plugin=protoc-gen-ts_proto --ts_proto_out=src/types ./proto/*.proto",
    "security:audit": "npm audit && snyk test",
    "docker:build": "docker build -t user-service --no-cache ."
  },
  "dependencies": {
    "@grpc/grpc-js": "1.9.x",
    "@grpc/proto-loader": "0.7.x",
    "@hapi/boom": "10.0.1",
    "ajv": "8.12.0",
    "ajv-formats": "2.1.1",
    "bcryptjs": "^2.4.3",
    "circuit-breaker-js": "^0.5.0",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "dayjs": "1.x",
    "dotenv": "^16.3.1",
    "express": "4.18.2",
    "express-rate-limit": "7.1.0",
    "express-validator": "7.0.1",
    "helmet": "7.1.0",
    "http-status-codes": "2.3.0",
    "ioredis": "5.x",
    "jsonwebtoken": "9.0.0",
    "mongoose": "7.x",
    "morgan": "1.10.0",
    "prom-client": "14.x",
    "rate-limit-redis": "4.0.x",
    "redis": "4.6.7",
    "winston": "3.10.0",
    "ws": "8.13.0",
    "xss": "1.0.14"
  },
  "devDependencies": {
    "@grpc/mock": "1.0.0",
    "@types/bcryptjs": "^2.4.4",
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/morgan": "^1.9.5",
    "@types/node": "^18.17.15",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "ioredis-mock": "8.x",
    "jest": "29.x",
    "mongodb-memory-server": "8.x",
    "snyk": "^1.1234.5",
    "supertest": "6.x",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.0.4"
  }
}